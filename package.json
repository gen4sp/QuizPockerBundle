{
    "name": "quizpokerbundle",
    "version": "1.0.1",
    "description": "QuizPoker Bundle TypeScript project",
    "type": "module",
    "main": "dist/lib.js",
    "module": "dist/lib.js",
    "types": "dist/lib.d.ts",
    "exports": {
        ".": {
            "import": "./dist/lib.js",
            "types": "./dist/lib.d.ts"
        }
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsx src/index.ts",
        "start": "node dist/index.js",
        "watch": "tsc --watch",
        "clean": "rm -rf dist",
        "test": "vitest run",
        "test:watch": "vitest",
        "prepare": "yarn build",
        "prepublishOnly": "yarn build"
    },
    "keywords": [
        "typescript",
        "quiz",
        "poker"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@types/express": "^5.0.3",
        "@types/node": "^20.10.0",
        "tsx": "^4.6.0",
        "typescript": "^5.3.0",
        "vitest": "^1.0.0"
    },
    "dependencies": {
        "express": "^5.1.0",
        "socket.io": "^4.8.1"
    },
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=16.0.0"
    }
}
